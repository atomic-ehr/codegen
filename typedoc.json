{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@atomic-ehr/codegen",
  "tsconfig": "./tsconfig.typedoc.json",
  "skipErrorChecking": true,
  "entryPoints": [
    "./src/index.ts",
    "./src/config.ts",
    "./src/api/index.ts",
    "./src/typeschema/generator.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "./docs/api",
  "theme": "default",
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core API",
    "Generators",
    "Configuration",
    "Validation",
    "Utilities",
    "*"
  ],
  "sort": ["source-order"],
  "exclude": [
    "**/node_modules/**",
    "**/test/**",
    "**/tests/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/examples/**",
    "**/dist/**",
    "**/generated/**"
  ],
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "./README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "excludeExternals": true,
  "disableSources": false,
  "gitRevision": "main",
  "gitRemote": "origin",
  "hideGenerator": true,
  "searchInComments": true,
  "cleanOutputDir": true,
  "titleLink": "https://github.com/atomic-ehr/codegen",
  "navigationLinks": {
    "GitHub": "https://github.com/atomic-ehr/codegen",
    "Documentation": "https://atomic-ehr.github.io/codegen/",
    "Examples": "https://github.com/atomic-ehr/codegen/tree/main/examples"
  },
  "sidebarLinks": {
    "Getting Started": "https://atomic-ehr.github.io/codegen/guides/getting-started/",
    "Tutorials": "https://atomic-ehr.github.io/codegen/tutorials/",
    "FHIR Usage": "https://atomic-ehr.github.io/codegen/guides/fhir-usage/",
    "Migration Guide": "https://atomic-ehr.github.io/codegen/guides/migration/"
  }
}

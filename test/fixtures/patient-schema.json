{
  "identifier": {
    "name": "Patient",
    "kind": "resource",
    "url": "http://hl7.org/fhir/Patient",
    "package": "hl7.fhir.r4.core",
    "version": "4.0.1"
  },
  "description": "Demographics and other administrative information about an individual or animal receiving care or other health-related services.",
  "fields": {
    "id": {
      "type": {
        "name": "id",
        "kind": "primitive-type",
        "url": "http://hl7.org/fhir/id",
        "package": "hl7.fhir.r4.core",
        "version": "4.0.1"
      },
      "required": false,
      "description": "Logical id of this artifact"
    },
    "meta": {
      "type": {
        "name": "Meta",
        "kind": "complex-type",
        "url": "http://hl7.org/fhir/Meta",
        "package": "hl7.fhir.r4.core",
        "version": "4.0.1"
      },
      "required": false,
      "description": "Metadata about the resource"
    },
    "name": {
      "type": {
        "name": "HumanName",
        "kind": "complex-type",
        "url": "http://hl7.org/fhir/HumanName",
        "package": "hl7.fhir.r4.core",
        "version": "4.0.1"
      },
      "array": true,
      "required": false,
      "description": "A name associated with the patient"
    },
    "gender": {
      "type": {
        "name": "code",
        "kind": "primitive-type",
        "url": "http://hl7.org/fhir/code",
        "package": "hl7.fhir.r4.core",
        "version": "4.0.1"
      },
      "enum": ["male", "female", "other", "unknown"],
      "required": false,
      "description": "Administrative Gender - male | female | other | unknown"
    },
    "birthDate": {
      "type": {
        "name": "date",
        "kind": "primitive-type",
        "url": "http://hl7.org/fhir/date",
        "package": "hl7.fhir.r4.core",
        "version": "4.0.1"
      },
      "required": false,
      "description": "The date of birth for the individual"
    },
    "active": {
      "type": {
        "name": "boolean",
        "kind": "primitive-type",
        "url": "http://hl7.org/fhir/boolean",
        "package": "hl7.fhir.r4.core",
        "version": "4.0.1"
      },
      "required": false,
      "description": "Whether this patient's record is in active use"
    }
  }
}
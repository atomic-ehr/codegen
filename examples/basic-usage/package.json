{
  "name": "atomic-codegen-basic-usage",
  "version": "1.0.0",
  "description": "Basic usage examples for @atomic-ehr/codegen",
  "type": "module",
  "scripts": {
    "generate:fhir": "bun run generate-fhir-types.ts",
    "generate:uscore": "bun run generate-uscore-types.ts",
    "generate:custom": "bun run generate-custom-types.ts",
    "generate:all": "bun run generate:fhir && bun run generate:uscore && bun run generate:custom",
    "test": "bun test",
    "clean": "rm -rf generated/"
  },
  "dependencies": {
    "@atomic-ehr/codegen": "workspace:*"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.0.0"
  }
}
